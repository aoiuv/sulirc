  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "failFast": true,
    "verbose": true,
    "require": [
      "@babel/register",
      "ts-node/register",
      "babel-polyfill"
    ],
    "babel": {
      "testOptions": {
        "presets": [
          "@babel/preset-env"
        ]
      }
